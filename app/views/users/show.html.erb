<h1><%= @user.first_name %> <%= @user.last_name %>'s Playlist</h1>
<h4><a href="/">Dashboard</a></h4>
<h4><a href="/logout">Logout</a></h4>
<table class="striped centered">
  <thead>
    <th>Artist</th>
    <th>Title</th>
    <th>Times Added</th>
  </head>
  <tbody>
    <% @songs.each do |song|%>
      <tr>
        <td><%= song.song.artist %></td>
        <td><a href="/songs/<%= song.song_id %>"><%= song.song.title %></a></td>
        <td><%= @count[song.song_id] %></td>
      </tr>
    <% end %>
  </tbody>
</table>